<% provide(:title,t(:edit)) %>
<h1><%= t(:update) %></h1>
<div class="row">
  <div class="span6 offset3">
    <%= form_for(@student) do |f| %>
        <%= render 'shared/error_messages', object: f.object %>
        <%=  render 'fields', f: f %>
        <%= f.submit t(:save), class: "btn btn-large btn-primary" %>
    <% end %>


  </div>
</div>
