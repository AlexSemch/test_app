<% provide(:title, t(:edit_user)) %>
<h1><%= t(:edit_user) %></h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@student) do |f| %>
        <%= render 'shared/error_messages' , object: f.object %>
        <%= render 'fields', f: f %>
        <%= f.submit "Create", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
